{% extends "base.html.twig" %}

{% block body %}


<div class="container">
<br/>
<br/>
    <div class="row">
        <div class="col-sm-6">
        <h2>Type test cases</h2>
            <div class="form-horizontal">
                {{ form_start(form) }}
                <br/>
                <div class="form-group">
                    {{ form_row(form.testCase1) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase2) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase3) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase4) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase5) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase6) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase7) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase8) }}
                </div> 
                <div class="form-group">
                    {{ form_row(form.testCase9) }}
                </div>
                <div class="form-group">
                    {{ form_row(form.testCase10) }}
                </div>
                <div class="form-group">        
                    <div class="col-sm-offset-2 col-sm-10">
                        {{ form_row(form.save) }}
                    </div>
                </div>
                {{ form_end(form) }}
            </div>
        </div>
        <div class="col-sm-6">
            <h3> Results </h3>
            <br/>
            <div class="row"> 
                {{ result|nl2br }}
            </div>
    </div>
</div>

{% endblock %}
